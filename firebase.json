{
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/api/**", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/callout",
        "run": {
          "serviceId": "getquery",
          "region": "us-central1"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
